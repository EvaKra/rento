
<div class="container">
<h2>List an item </h2>


  <%= form_for @item  do |f| %>
     <%= f.label :title %>
     <%= f.text_field :title %>
     <%= f.label :description  %>
     <%= f.text_field :description %>
     <%= f.label :price %>
     <%= f.number_field :price %>
     <%= f.submit%>

  <% end %>

<%= link_to "back", root_path %>
</div>

